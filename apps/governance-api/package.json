{
  "name": "@trustful-agents/governance-api",
  "version": "1.0.0",
  "description": "Governance dashboard backend API with SIWE auth, Safe SDK, and email notifications",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@safe-global/api-kit": "^2.5.4",
    "@safe-global/protocol-kit": "^5.0.4",
    "@safe-global/safe-core-sdk-types": "^5.1.0",
    "@aws-sdk/client-ses": "^3.700.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "pg": "^8.13.1",
    "siwe": "^2.3.2",
    "viem": "^2.21.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
